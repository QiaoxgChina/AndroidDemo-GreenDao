<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/color_app_bg"
              android:orientation="vertical">

    <include
        android:id="@+id/result_include_titleBar"
        layout="@layout/view_title_bar"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:background="@drawable/selector_item_bg"
        android:clickable="true"
        android:paddingLeft="10dp"
        android:paddingRight="10dp">

        <LinearLayout
            android:id="@+id/result_ll_shuangseqiuinfo"
            android:layout_width="match_parent"
            android:layout_height="30dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="bottom"
                android:text="双色球"
                android:textSize="16dp"
                android:textColor="#000"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="4dp"
                android:gravity="bottom"
                android:text="第001期"
                android:textColor="@color/color_9D9D9D"
                android:textSize="12dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="4dp"
                android:gravity="bottom"
                android:text="04-05(周三)"
                android:textColor="@color/color_9D9D9D"
                android:textSize="12dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_below="@+id/result_ll_shuangseqiuinfo">

            <TextView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:background="@drawable/shape_bg_redball_selected"
                android:gravity="center"
                android:text="01"
                android:textColor="@color/color_FFFFFF"/>

            <TextView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:background="@drawable/shape_bg_redball_selected"
                android:gravity="center"
                android:text="04"
                android:textColor="@color/color_FFFFFF"/>

            <TextView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:background="@drawable/shape_bg_redball_selected"
                android:gravity="center"
                android:text="08"
                android:textColor="@color/color_FFFFFF"/>

            <TextView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:background="@drawable/shape_bg_redball_selected"
                android:gravity="center"
                android:text="13"
                android:textColor="@color/color_FFFFFF"/>

            <TextView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:background="@drawable/shape_bg_redball_selected"
                android:gravity="center"
                android:text="24"
                android:textColor="@color/color_FFFFFF"/>

            <TextView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:background="@drawable/shape_bg_redball_selected"
                android:gravity="center"
                android:text="27"
                android:textColor="@color/color_FFFFFF"/>

            <TextView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:background="@drawable/shape_bg_blueball_selected"
                android:gravity="center"
                android:text="05"
                android:textColor="@color/color_FFFFFF"/>
        </LinearLayout>

        <ImageView
            android:id="@+id/label"
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/btn_right"/>

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#00000000"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:background="@drawable/selector_item_bg"
        android:clickable="true"
        android:orientation="vertical"
        android:paddingLeft="10dp"
        android:paddingRight="10dp">

        <LinearLayout
            android:id="@+id/result_ll_info"
            android:layout_width="match_parent"
            android:layout_height="29dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="bottom"
                android:text="双色球"
                android:textColor="#000"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="4dp"
                android:gravity="bottom"
                android:text="第001期"
                android:textColor="@color/color_9D9D9D"
                android:textSize="12dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="4dp"
                android:gravity="bottom"
                android:text="04-05(周三)"
                android:textColor="@color/color_9D9D9D"
                android:textSize="12dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="46dp"
            android:layout_below="@+id/result_ll_info">

            <TextView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:background="@drawable/shape_bg_redball_selected"
                android:gravity="center"
                android:text="01"
                android:textColor="@color/color_FFFFFF"/>

            <TextView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:background="@drawable/shape_bg_redball_selected"
                android:gravity="center"
                android:text="04"
                android:textColor="@color/color_FFFFFF"/>

            <TextView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:background="@drawable/shape_bg_redball_selected"
                android:gravity="center"
                android:text="08"
                android:textColor="@color/color_FFFFFF"/>

            <TextView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:background="@drawable/shape_bg_redball_selected"
                android:gravity="center"
                android:text="13"
                android:textColor="@color/color_FFFFFF"/>

            <TextView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:background="@drawable/shape_bg_redball_selected"
                android:gravity="center"
                android:text="24"
                android:textColor="@color/color_FFFFFF"/>

            <TextView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:background="@drawable/shape_bg_redball_selected"
                android:gravity="center"
                android:text="27"
                android:textColor="@color/color_FFFFFF"/>

            <TextView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:background="@drawable/shape_bg_blueball_selected"
                android:gravity="center"
                android:text="05"
                android:textColor="@color/color_FFFFFF"/>
        </LinearLayout>

        <ImageView
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/btn_right"/>

    </RelativeLayout>

</LinearLayout>